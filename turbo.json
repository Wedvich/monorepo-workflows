{
  "$schema": "./node_modules/turbo/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "test"],
      "outputs": ["dist/**"],
      "outputLogs": "new-only"
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "outputLogs": "new-only"
    }
  }
}
